<div class="user-details form-group">
    <h3 type="text">
    Name: <%=@name%>
    </h3>

    <h3 type="email"> 
    Email: <%=@email%>
    </h3>
</div>

<%= 
if @authenticated && @authenticated_uni.strip != @user.strip
  if !@following
    button_to "Follow", create_follower_path(@user, @authenticated_uni), method: :put, class: "btn btn-primary mb-4", id: "follow_button" 
  else
    button_to "Unfollow", delete_follower_path(@user, @authenticated_uni), method: :delete, class: "btn btn-primary mb-4", id: "follow_button"
  end 
end
%>

</form>
<a href="./<%@user%>/profile/followers">
<button type="button" class="btn btn-secondary">Followers</button>

</a>
<a href="./<%@user%>/profile/following">
<button type="button" class="btn btn-secondary">Following</button>

</a>

<h3>Organizing</h3>
<div class="container" style="margin: 10 0; padding: 0;" >
    <div class="events row">
      <% @myevents.each_with_index do |event, i| %>
      <div class="card col-md-4" style="width: 18rem; margin: 10px 20px;">
        <img class="card-img-top" src="data:image/jpeg;base64," alt="Card image cap">
        <div class="card-body">
          <h5 class="card-title"><%= link_to event.title, event_path(@user, event.id) %></h6>
          <h6 class="card-title"><span>On: <%= event.start_date %></h6>
          <p class="card-text"> <%= event.description%> </p>
          <h7 class="card-title">Organized by: <%= link_to event.organizer, profile_path(event.organizer)%></h7>
          <!-- a href="#/i/registration" class="btn btn-primary">Get Tickets</a -->
        </div>
      </div>
      <% end %>
    </div>
</div>

<h3>Attending Events</h3>
<div class="container" style="margin: 10 0; padding: 0;" >
    <div class="events row">
      <% @attending_events.each_with_index do |event, i| %>
      <div class="card col-md-4" style="width: 18rem; margin: 10px 20px;">
        <img class="card-img-top" src="data:image/jpeg;base64," alt="Card image cap">
        <div class="card-body">
          <h5 class="card-title"><%= link_to event.title, event_path(@user, event.id) %></h6>
          <h6 class="card-title"><span>On: <%= event.start_date %></h6>
          <p class="card-text"> <%= event.description%> </p>
          <h7 class="card-title">Organized by: <%= link_to event.organizer, profile_path(event.organizer)%></h7>
          <!-- a href="#/i/registration" class="btn btn-primary">Get Tickets</a -->
        </div>
      </div>
      <% end %>
    </div>
</div>